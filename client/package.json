{
  "name": "starhackit-ui",
  "title": "StarHack.It",
  "version": "7.0.0",
  "description": "React + node starter kit. A fullstack boilerplate written in ES6/7 with authentication and authorisation",
  "homepage": "https://github.com/FredericHeem/starhackit",
  "license": "NLPL",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open --hot",
    "start:prod": "http-server dist/",
    "prestart": "npm run prebuild",
    "lint": "tslint -c tslint.json './src/**/*.{js,jsx,ts,tsx}'",
    "cpd": "jscpd",
    "prebuild": "npm run lint && npm run cpd",
    "build": "webpack --config webpack.prod.js --progress",
    "clean": "rimraf dist coverage",
    "test": "karma start --single-run --no-auto-watch karma.conf.js",
    "test:watch": "karma start --auto-watch --no-single-run karma.conf.js",
    "selenium-install": "webdriver-manager update",
    "e2e": "npm run nightwatch",
    "nightwatch": "cd test/nightwatch/ && nightwatch",
    "docker:build": "npm run build && docker build -t starhackit/ui .",
    "docker:run": "docker run --rm -p 80:80 -p 443:443  --name starhackit-ui starhackit/ui",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "bugs": {
    "url": "https://github.com/FredericHeem/starhackit/issues"
  },
  "browserslist": "last 2 versions, ie 11",
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@emotion/styled": "10.0.12",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.20",
    "@types/styled-jsx": "2.2.8",
    "axios": "0.19.0",
    "debug": "4.1.1",
    "history": "4.9.0",
    "i18next": "17.0.3",
    "i18next-browser-languagedetector": "3.0.1",
    "i18next-localstorage-cache": "1.1.1",
    "i18next-xhr-backend": "3.0.0",
    "intl": "1.2.5",
    "lodash": "4.17.11",
    "mdlean": "0.16.0",
    "mobx": "5.10.1",
    "mobx-react": "6.1.1",
    "preact-deep-force-update": "0.1.0",
    "prop-types": "15.7.2",
    "qs": "6.7.0",
    "react": "16.8.6",
    "react-color": "2.17.3",
    "react-dom": "16.8.6",
    "react-leanimate": "0.16.0",
    "react-pagify": "2.4.0",
    "reactabular-table": "8.14.0",
    "segmentize": "0.4.1",
    "universal-router": "8.2.0",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "bluebird": "3.5.5",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chance": "1.0.18",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "3.0.0",
    "file-loader": "4.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "imports-loader": "0.8.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jscpd": "2.0.15",
    "json-loader": "0.5.7",
    "karma": "3.1.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.5",
    "lodash-webpack-plugin": "0.11.5",
    "mini-css-extract-plugin": "0.7.0",
    "mocha": "6.1.4",
    "nightwatch": "1.1.12",
    "phantomjs-prebuilt": "2.1.16",
    "protractor": "5.4.2",
    "puppeteer": "1.11.0",
    "sinon": "7.3.2",
    "style-loader": "0.23.1",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.3",
    "ts-node": "8.3.0",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-react": "4.0.0",
    "typescript": "3.5.2",
    "url-loader": "2.0.0",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-bundle-size-analyzer": "3.0.0",
    "webpack-cli": "3.3.4",
    "webpack-dev-server": "3.7.2",
    "webpack-hud": "0.1.2"
  },
  "engines": {
    "node": ">8.x",
    "npm": ">5.x"
  },
  "false": {},
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat",
    "mobx-react": "mobx-preact",
    "create-react-class": "preact-compat/lib/create-react-class"
  }
}
