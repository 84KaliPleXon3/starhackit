{
  "name": "starhackit-ui",
  "title": "StarHack.It",
  "version": "5.13.0",
  "description": "React + node starter kit. A fullstack boilerplate written in ES6/7 with authentication and authorisation",
  "homepage": "https://github.com/FredericHeem/starhackit",
  "license": "NLPL",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open --hot",
    "start:prod": "http-server dist/",
    "prestart": "npm run prebuild",
    "lint": "tslint -c tslint.json './src/**/*.{js,jsx,ts,tsx}'",
    "cpd": "jscpd",
    "prebuild": "npm run lint && npm run cpd",
    "build": "webpack --config webpack.prod.js --progress",
    "clean": "rimraf dist coverage",
    "test": "karma start --single-run --no-auto-watch karma.conf.js",
    "test:watch": "karma start --auto-watch --no-single-run karma.conf.js",
    "selenium-install": "webdriver-manager update",
    "e2e": "npm run nightwatch",
    "nightwatch": "cd test/nightwatch/ && nightwatch",
    "docker:build": "npm run build && docker build -t starhackit/ui .",
    "docker:run": "docker run --rm -p 80:80 -p 443:443  --name starhackit-ui starhackit/ui",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "bugs": {
    "url": "https://github.com/FredericHeem/starhackit/issues"
  },
  "browserslist": "last 2 versions, ie 11",
  "dependencies": {
    "@types/jest": "23.3.10",
    "axios": "0.18.0",
    "debug": "4.1.0",
    "glamor": "2.20.40",
    "glamorous": "4.13.1",
    "history": "4.7.2",
    "i18next": "12.0.0",
    "i18next-browser-languagedetector": "2.2.3",
    "i18next-localstorage-cache": "1.1.1",
    "i18next-xhr-backend": "1.5.1",
    "intl": "1.2.5",
    "lodash": "4.17.11",
    "mdlean": "0.15.0",
    "mobx": "5.5.2",
    "mobx-preact": "3.0.0",
    "preact": "8.3.1",
    "preact-compat": "3.18.4",
    "preact-deep-force-update": "0.1.0",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react-color": "2.14.1",
    "react-leanimate": "0.16.0",
    "react-pagify": "2.4.0",
    "reactabular-table": "8.14.0",
    "segmentize": "0.4.1",
    "universal-router": "7.0.0",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.19",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "bluebird": "3.5.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chance": "1.0.16",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "4.5.4",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "imports-loader": "0.8.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jscpd": "0.6.25",
    "json-loader": "0.5.7",
    "karma": "3.1.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.5",
    "lodash-webpack-plugin": "0.11.5",
    "mini-css-extract-plugin": "0.4.4",
    "mocha": "5.2.0",
    "nightwatch": "0.9.21",
    "phantomjs-prebuilt": "2.1.16",
    "protractor": "5.4.1",
    "sinon": "7.1.0",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-loader": "3.5.4",
    "tslint-react": "3.6.0",
    "typescript": "3.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.23.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-bundle-size-analyzer": "2.7.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-hud": "0.1.2"
  },
  "engines": {
    "node": ">4.x",
    "npm": ">3.x"
  },
  "false": {},
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat",
    "mobx-react": "mobx-preact",
    "create-react-class": "preact-compat/lib/create-react-class"
  }
}
