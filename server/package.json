{
  "name": "starhackit-api",
  "description": "The starhackit backend",
  "version": "9.0.4",
  "private": false,
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "license": "NLPL",
  "scripts": {
    "prestart": "npm run lint && npm run jscpd",
    "build": "",
    "start": "NODE_PATH=src nodemon -e ts,js,graphql -x node src/index.js",
    "pretest": "npm run build",
    "test": "npm run coverage",
    "mocha": "NODE_PATH=src mocha",
    "coverage": "NODE_PATH=src istanbul  cover -x 'src/**/test*.js' -x 'test/**/test*.js' -x 'src/plugins/crossbank/*' -x 'src/plugins/dbSchema/*'  -x 'src/plugins/ticket/*' -x 'src/plugins/document/*' -x 'src/plugins/fidor/*' _mocha --report text --report html --report lcov ./src/**/test*.js ./test/**/*.js",
    "coveralls": "coveralls < coverage/lcov.info",
    "codecov": "./node_modules/codecov.io/bin/codecov.io.js < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start:prod": "pm2 start pm2.json --no-daemon",
    "clean": "",
    "mocker": "node scripts/mocker-server.js",
    "doc": "node scripts/apidoc.js",
    "opendoc": "npm run doc && open build/api/v1/doc/api.html",
    "jscpd": "jscpd -f 'src/**'",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:recreate": "npm run db:drop && npm run db:create && npm run db:migrate",
    "docker:build": "docker build . -t api",
    "docker:tag": "docker tag api fredericheem/api:latest",
    "docker:push": "docker push fredericheem/api:latest",
    "docker:btp": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:destroy": "docker ps -a -q | xargs docker rm -f && docker volume prune -f",
    "setup": "npm run docker:down  && npm run docker:up && sleep 1 && npm run db:migrate"
  },
  "dependencies": {
    "async-busboy": "0.7.0",
    "axios": "0.19.0",
    "bcryptjs": "2.4.3",
    "bluebird": "3.5.5",
    "chance": "1.0.18",
    "config": "3.2.2",
    "ejs": "2.6.2",
    "import-to-commonjs": "0.2.5",
    "jsonschema": "1.2.4",
    "jsonwebtoken": "8.5.1",
    "kcors": "2.2.2",
    "koa": "2.8.1",
    "koa-66": "1.0.1",
    "koa-bodyparser": "4.2.1",
    "koa-generic-session": "2.0.1",
    "koa-passport": "4.1.3",
    "koa-redis": "4.0.0",
    "koa-static": "5.0.0",
    "lodash": "4.17.15",
    "logfilename": "0.3.0",
    "moment": "2.24.0",
    "nodemailer": "6.3.0",
    "oauth": "0.9.15",
    "passport-facebook": "3.0.0",
    "passport-google-id-token": "0.4.6",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "passport-oauth2": "1.5.0",
    "pg": "7.4.1",
    "pg-hstore": "2.3.2",
    "pm2": "3.5.1",
    "qs": "6.8.0",
    "redis": "2.8.0",
    "sequelize": "4.41.2",
    "sequelize-cli": "5.4.0",
    "urijs": "1.19.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "async": "3.1.0",
    "chai": "4.2.0",
    "codeclimate-test-reporter": "0.5.1",
    "codecov.io": "0.1.6",
    "coveralls": "3.0.6",
    "eslint": "6.3.0",
    "eslint-plugin-mocha": "6.1.0",
    "faker": "4.1.0",
    "isparta": "4.1.1",
    "istanbul": "0.4.5",
    "jscpd": "2.0.15",
    "mocha": "6.2.0",
    "nodemon": "1.19.1",
    "raml-mocker-server": "0.1.6",
    "raml2html": "7.4.0",
    "request": "2.88.0",
    "shelljs": "0.8.3",
    "should": "13.2.3",
    "sinon": "7.4.1"
  },
  "engines": {
    "node": ">=8"
  }
}
