{
  "name": "starhackit-api",
  "version": "0.0.1",
  "private": false,
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "license" : "NLPL",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "./node_modules/.bin/gulp build:production",
    "start": "./node_modules/.bin/gulp",
    "pretest": "npm run build",
    "test": "npm run coverage",
    "mocha": "./node_modules/.bin/mocha ./src/**/test*.js ./test/**/test*.js",
    "coverage": "./node_modules/.bin/istanbul  cover -x '**/test*.js'  _mocha --report text --report html --report lcov ./src/**/test*.js ./test/**/*.js",
    "coveralls": "./node_modules/.bin/coveralls < coverage/lcov.info",
    "codecov": "./node_modules/codecov.io/bin/codecov.io.js < coverage/lcov.info",
    "codeclimate": "./node_modules/.bin/codeclimate-test-reporter < coverage/lcov.info",
    "lint": "./node_modules/.bin/eslint src",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "postinstall": "npm run build",
    "nodemon": "./node_modules/.bin/nodemon",
    "run": "node build/index.js"
  },
